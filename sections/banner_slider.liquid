{% for banner in section.blocks %}
  <img src="{{ banner.settings.image }}" height="100" width="100">
{% endfor %}
{% schema %}
{
  "name": "Banner Slider",
  "blocks": [
  {
    "type": "slide",
    "name": "Banner Slider",
    "limit": 3,
    "settings": [
      {
        "type": "image_picker",
        "id": "image",
        "label": "Select Background Image"
      },
      {
        "type": "url",
        "id": "link",
        "label": "Banner Link"
      }
    ]
  }
]
}
{% endschema %}
